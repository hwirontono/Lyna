<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    
    @HTMLHelper.StandardCSS()
    @HTMLHelper.StandardJS()
    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="../assets/js/html5shiv.js"></script>
    <![endif]-->
  </head>
 
  <body>
    <div id="wrap">
        <div class="navbar navbar-inverse navbar-fixed-top">
          <div class="navbar-inner">
            <div class="container">
              <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
              </button>
              <a class="brand" href="../">Dollar Xpress</a>
              <div class="nav-collapse collapse">
                <ul class="nav">
                  <li class="active"><a href="../Order/">Invoice</a></li>
                  <li><a href="#about">Customer</a></li>
                  <li><a href="#about">Report</a></li>
                  <li class="dropdown hide">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Report <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                      <li><a href="#">Action</a></li>
                      <li><a href="#">Another action</a></li>
                    </ul>
                  </li>
                </ul>
                <form class="navbar-form pull-right hide">
                  <input class="span2" type="text" placeholder="Email">
                  <input class="span2" type="password" placeholder="Password">
                  <button type="submit" class="btn">Sign in</button>
                </form>
              </div><!--/.nav-collapse -->
            </div>
          </div>
        </div>
 
        <div class="container">
          @RenderBody()
          <hr/>
          
        </div> <!-- /container -->
        <div id="push"></div>
    </div>

    <div id="footer">
        <div class="container">
            <p class="muted credit">
                Copyright 2013 - Dollar Express
            </p>
        </div>
    </div>

    <!-- Placed at the end of the document so the pages load faster -->
   
    @RenderSection("scripts", required: false)
  </body>
</html>

